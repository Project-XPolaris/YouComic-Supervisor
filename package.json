{
  "name": "ant-design-pro",
  "version": "1.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run site && npm run gh-pages",
    "fetch:blocks": "pro fetch-blocks --branch antd@4 && npm run prettier",
    "format-imports": "cross-env import-sort --write '**/*.{js,jsx,ts,tsx}'",
    "gh-pages": "cp CNAME ./dist/ && gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc",
    "ui": "umi ui",
    "build:electron:prod": "cross-env NODE_ENV=production webpack --config ./build/webpack.main.prod.config.js",
    "build:electron:dev": "cross-env NODE_ENV=development webpack --config ./build/webpack.main.config.js",
    "start:electron": "cross-env NODE_ENV=development electron-webpack dev",
    "build:win-portable": "electron-builder --win portable",
    "pack": "electron-builder --dir"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/compatible": "^1.0.2",
    "@ant-design/icons": "^4.0.5",
    "@ant-design/pro-cli": "^1.0.17",
    "@ant-design/pro-layout": "^6.18.2",
    "ahooks": "^2.7.1",
    "ant-design-pro": "^2.3.2",
    "antd": "^4.15.6",
    "antd-img-crop": "^2.4.0",
    "awesome-typescript-loader": "^5.2.1",
    "classnames": "^2.2.6",
    "dva-logger": "^1.0.0",
    "jwt-simple": "^0.5.6",
    "localforage": "^1.7.3",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "omit.js": "^1.0.2",
    "path-to-regexp": "2.4.0",
    "qs": "^6.9.0",
    "react": "^16.8.6",
    "react-beautiful-dnd": "^13.0.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.6",
    "react-easy-crop": "^2.0.0",
    "react-gesture-responder": "^2.1.0",
    "react-grid-dnd": "^2.1.2",
    "react-helmet": "^5.2.1",
    "react-image-crop": "^8.5.0",
    "react-image-file-resizer": "^0.2.1",
    "umi": "^3.4.22",
    "umi-request": "^1.0.8",
    "urijs": "^1.19.2",
    "use-merge-value": "^1.0.1"
  },
  "devDependencies": {
    "@antv/data-set": "^0.10.2",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.23",
    "@types/jwt-simple": "^0.5.33",
    "@types/localforage": "^0.0.34",
    "@types/lodash": "^4.14.144",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.8.19",
    "@types/react-beautiful-dnd": "^12.1.2",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.13",
    "@types/react-image-crop": "^8.1.2",
    "@types/urijs": "^1.19.5",
    "@umijs/fabric": "^2.0.5",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/preset-ant-design-pro": "^1.0.1",
    "@umijs/preset-react": "^1.4.19",
    "@umijs/preset-ui": "^2.0.9",
    "chalk": "^3.0.0",
    "check-prettier": "^1.0.3",
    "copy-webpack-plugin": "^5.1.2",
    "cross-env": "^6.0.0",
    "cross-port-killer": "^1.1.1",
    "electron": "^11.4.8",
    "electron-builder": "^22.10.5",
    "electron-debug": "^3.0.1",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "enzyme": "^3.9.0",
    "express": "^4.17.1",
    "gh-pages": "^2.0.1",
    "husky": "^4.0.7",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest-puppeteer": "^4.2.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^9.0.0",
    "mockjs": "^1.0.1-beta3",
    "node-fetch": "^2.6.0",
    "prettier": "^1.19.1",
    "pro-download": "1.0.1",
    "stylelint": "^13.0.0",
    "ts-loader": "^9.2.3",
    "webpack": "4.28.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.1"
  },
  "optionalDependencies": {
    "puppeteer": "^2.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "electronDownload": {
    "mirror": "https://npm.taobao.org/mirrors/electron/"
  },
  "electronWebpack": {
    "main": {
      "sourceDirectory": "main"
    },
    "renderer": {
      "sourceDirectory": null
    }
  },
  "main": "./dist/main/main.js",
  "build": {
    "appId": "com.vincentx.umi-electron-biolerplate",
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "directories": {
      "output": "release"
    }
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
